{
  "Routes": [
    {
      "ServiceName": "insurant",
      "DownstreamPathTemplate": "/info/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/insurant/{everything}",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{RemoteIpAddress}",
        "Host": "localhost:5000, app.pr114.isago.ch"
      },
      "DelegatingHandlers": [
        "HostInjectorDelegatingHandler"
      ]
    },
    {
      "ServiceName": "invoice",
      "DownstreamPathTemplate": "/info/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/invoice/{everything}",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{RemoteIpAddress}",
        "Host": "localhost:5000, app.pr114.isago.ch"
      },
      "DelegatingHandlers": [
        "HostInjectorDelegatingHandler"
      ]
    },
    {
      "ServiceName": "benefit",
      "DownstreamPathTemplate": "/info/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/benefit/{everything}",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{RemoteIpAddress}",
        "Host": "localhost:5000, app.pr114.isago.ch"
      },
      "DelegatingHandlers": [
        "HostInjectorDelegatingHandler"
      ]
    },
    {
      "DownstreamPathTemplate": "/info/env",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/env",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "DownstreamHostAndPorts": [
        {
          "Host": "app1",
          "Port": 443
        }
      ],
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{RemoteIpAddress}",
        "Host": "localhost:5000, app.pr114.isago.ch"
      },
      "DelegatingHandlers": [
        "HostInjectorDelegatingHandler"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000",
    "ServiceDiscoveryProvider": {
      "Host": "rp01",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}